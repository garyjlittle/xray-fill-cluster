[global]
ioengine=libaio
direct=1
norandommap
group_reporting
disk_util=0
continue_on_error=all
rate_process=poisson

[db-oltp1]
bs=1m
size=100%
filename=/dev/sdb
rw=write
iodepth=8

[db-oltp2]
bs=1m
size=100%
filename=/dev/sdc
rw=write
iodepth=8

[db-oltp3]
bs=1m
size=100%
filename=/dev/sdd
rw=write
iodepth=8

[db-oltp4]
bs=1m
size=100%
filename=/dev/sde
rw=write
iodepth=8

